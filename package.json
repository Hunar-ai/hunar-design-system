  "version": "0.1.0",
  "description": "Hunar's Design System",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Hunar-ai/hunar-design-system.git"
  },
  "author": "Hunar <tech@hunar.ai>",
  "bugs": {
